{
  "description": "O seu queridinho Orkut com um novo visual no Tema Escuro.",
  "manifest_version": 2,
  "name": "Orkut Redesign Dark",
  "author": "Johnendz",
  "version": "1.4",

  "content_scripts": [
    {
      "matches": ["*://www.orkut.br.com/*"],
      "css":["style.minified.css"],
      "run_at": "document_start"
    }
  ],
  
  "icons": {
      "32": "icons/32.png",
      "64": "icons/64.png",
      "128": "icons/128.png"
  }
}